tado from \"./components/Listado\";\nimport Formulario from \"./components/Formulario\";\n\n\nfunction App() {\n\n  const [modal, setModal] = useState(false);\n\n  return (\n    <BalanceProvider>\n      <AlertasProvider>\n\n        <section className=\"application\">\n          <Navbar />\n\n          <div className=\"container\">\n            <Balance />\n            <Listado />\n\n            <Modal show={modal}>\n              <Modal.Header>\n\n                <h2 className=\"font-weight-bold\">Ingresa un movimiento</h2>\n                <button\n                  type=\"button\"\n                  className=\"close\"\n                  data-dismiss=\"modal\"\n                  aria-label=\"Close\"\n                  onClick={() => setModal(!modal)}>\n                  <span aria-hidden=\"true\">&times;</span>\n                </button>\n\n              </Modal.Header>\n\n              <Formulario modal={modal} setModal={setModal} />\n\n            </Modal>\n\n            <button className=\"btn btn-flotante\">\n              <h2 className=\"font-weight-bold\" onClick={() => setModal(!modal)}>Agregar Movimiento</h2>\n            </button>\n\n          </div>\n        </section>\n\n\n      </AlertasProvider>\n    </BalanceProvider>\n  );\n}\n\nexport default App;\n","C:\\Users\\daans\\Desktop\\Portafolio\\gestor-de-gastos\\gestorgastos\\src\\reportWebVitals.js",[],"C:\\Users\\daans\\Desktop\\Portafolio\\gestor-de-gastos\\gestorgastos\\src\\components\\Navbar.jsx",[],"C:\\Users\\daans\\Desktop\\Portafolio\\gestor-de-gastos\\gestorgastos\\src\\components\\Balance.jsx",["54"],"import React, { useContext, useEffect } from 'react';\r\n//Context\r\nimport { BalanceContext } from '../context/balanceContext';\r\n\r\nconst Balance = () => {\r\n\r\n    const { movimientos, ingresos, gastos, balance, actualizarBalance } = useContext(BalanceContext);\r\n\r\n    useEffect(() => {\r\n \r\n        if (movimientos.length !== null) {\r\n            actualizarBalance(ingresos, gastos);\r\n        }\r\n\r\n    }, [movimientos]);\r\n\r\n    return (\r\n\r\n        <div className=\"row justify-content-center\">\r\n            <div className=\"col-md-8 col-sm-9 mt-2\">\r\n                <div className=\"card\">\r\n                    <div className=\"card-body text-center pt-2 pb-2\">\r\n\r\n                        <p className=\"mb-1 \">Balance del mes:</p>\r\n\r\n                        {balance >= 0 ?\r\n                            (<h1 className=\"balance font-weight-bold mt-0\">${balance}</h1>) :\r\n                            (<h1 className=\"font-weight-bold text-danger\">-${balance.slice(1, balance.length)}</h1>)}\r\n\r\n                        <br />\r\n\r\n                        <div className=\"balance__stats row justify-content-around\">\r\n                            <div className=\"ingresos text-success\">\r\n                                <h2>Ingresos:</h2>\r\n\r\n                                {ingresos > 0 ? <p>+${ingresos}</p> : <p>Sin ingresos</p>}\r\n                                \r\n                            </div>\r\n                            <div className=\"gastos text-danger\">\r\n                                <h2>Gastos:</h2>\r\n\r\n                                {gastos > 0 ? ( <p>-${gastos}</p> ) : ( <p>Sin gastos</p>)}\r\n                               \r\n                            </div>\r\n                        </div>\r\n\r\n                        <button className=\"btn--decoration\">\r\n                            <p className=\"m-0 text-secondary\">Ver anal√≠ticas</p>\r\n                            <i className=\"fas fa-chevron-down\"></i>\r\n                        </button>\r\n\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    );\r\n}\r\n\r\nexport default Balance;","C:\\Users\\daans\\Desktop\\Portafolio\\gestor-de-gastos\\gestorgastos\\src\\components\\Listado.jsx",[],"C:\\Users\\daans\\Desktop\\Portafolio\\gestor-de-gastos\\gestorgastos\\