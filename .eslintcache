     movimientos,\r\n                ingresos,\r\n                gastos,\r\n                balance,\r\n                agregarMovimiento,\r\n                agregarIngreso,\r\n                agregarGasto,\r\n                actualizarBalance,\r\n                eliminarMovimiento\r\n            }}>\r\n\r\n            {props.children}\r\n        </BalanceContext.Provider>\r\n    )\r\n}\r\n\r\nexport default BalanceProvider;","C:\\Users\\daans\\Desktop\\Portafolio\\gestor-de-gastos\\gestorgastos\\src\\types\\index.js",[],"C:\\Users\\daans\\Desktop\\Portafolio\\gestor-de-gastos\\gestorgastos\\src\\context\\alertasContext.js",[],{"ruleId":"58","replacedBy":"59"},{"ruleId":"60","replacedBy":"61"},{"ruleId":"62","severity":1,"message":"63","line":1,"column":27,"nodeType":"64","messageId":"65","endLine":1,"endColumn":37},{"ruleId":"62","severity":1,"message":"66","line":6,"column":17,"nodeType":"64","messageId":"65","endLine":6,"endColumn":23},{"ruleId":"67","severity":1,"message":"68","line":15,"column":8,"nodeType":"69","endLine":15,"endColumn":21,"suggestions":"70"},{"ruleId":"62","severity":1,"message":"66","line":4,"column":17,"nodeType":"64","messageId":"65","endLine":4,"endColumn":23},{"ruleId":"62","severity":1,"message":"71","line":17,"column":31,"nodeType":"64","messageId":"65","endLine":17,"endColumn":37},{"ruleId":"62","severity":1,"message":"72","line":1,"column":27,"nodeType":"64","messageId":"65","endLine":1,"endColumn":36},"no-native-reassign",["73"],"no-negated-in-lhs",["74"],"no-unused-vars","'useContext' is defined but never used.","Identifier","unusedVar","'Button' is defined but never used.","react-hooks/exhaustive-deps","React Hook useEffect has missing dependencies: 'actualizarBalance', 'gastos', and 'ingresos'. Either include them or remove the dependency array.","ArrayExpression",["75"],"'estado' is assigned a value but never used.","'useEffect' is defined but never used.","no-global-assign","no-unsafe-negation",{"desc":"76","fix":"77"},"Update 